                <library dirs="200">
                    <file>${libs.Hibernate-3.6.2.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.HibernateValidator-4.1.0.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.HTMLCompressor-1.1.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
